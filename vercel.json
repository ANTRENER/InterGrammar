{
    "version": 2,
    "functions": {
        "api/webhook.ts": {
            "maxDuration": 10
        }
    },
    "headers": [
        {
            "source": "/api/webhook",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "POST, OPTIONS"
                }
            ]
        }
    ]
}